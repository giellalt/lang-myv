<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:hfp="http://www.w3.org/2001/XMLSchema-hasFacetAndProperty">
   <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
   <xs:element name="r">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="entry"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="entry">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="lg"/>
            <!--
      <xs:element ref="stem"/>
      <xs:element ref="contlex"/>
-->
            <xs:element ref="pos"/>
            <xs:element minOccurs="0" ref="phonet"/>
            <xs:element maxOccurs="unbounded" ref="e"/>
         </xs:sequence>
         <xs:attribute name="exclude">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="fst"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="order">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="1"/>
                  <xs:enumeration value="2"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="lg">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="l"/>
            <xs:element ref="stg"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="stg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="st"/>
            <xs:element ref="stCtn"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="l">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="pos"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="st">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="src"/>
         </xs:sequence>
         <xs:attribute ref="contlex"/>
         <xs:attribute ref="erzContlex"/>
         <xs:attribute name="stOrth">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="allegro"/>
                  <xs:enumeration value="dial"/>
                  <xs:enumeration value="erz"/>
                  <xs:enumeration value="Kozl"/>
                  <xs:enumeration value="old"/>
                  <xs:enumeration value="phon"/>
                  <xs:enumeration value="ru"/>
                  <xs:enumeration value="ry"/>
                  <xs:enumeration value="uncertain"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="stCtn">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="2" minOccurs="2" ref="st"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="src"/>
         </xs:sequence>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="hyphenated"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="contlex">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string"/>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="pos" type="posType"/>
   <xs:element name="phonet">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string"/>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="drv">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="drvType"/>
               <xs:attribute ref="lemmaID"/>
               <xs:attribute ref="erzMorph"/>
               <xs:attribute ref="der"/>
               <!--This should be moved into drvType where possible-->
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="etymg">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="hypoErz"/>
            <xs:element ref="etym"/>
            <xs:element ref="etymCtn"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="etymCtn">
      <xs:complexType>
         <xs:choice>
            <xs:element maxOccurs="unbounded" minOccurs="2" ref="etym"/>
         </xs:choice>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="stemVariation"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="etym">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tg"/>
         </xs:sequence>
         <xs:attribute ref="cx"/>
         <xs:attribute ref="dx"/>
         <xs:attribute ref="etymType"/>
         <xs:attribute ref="lang"/>
         <xs:attribute ref="lemmaID"/>
         <xs:attribute ref="nx"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="hypoErz">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" ref="constituent"/>
         </xs:sequence>
         <xs:attribute ref="constituentOrder"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="e">
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="drv"/>
            <xs:element minOccurs="0" ref="etymg"/>
            <xs:element maxOccurs="unbounded" ref="mg"/>
         </xs:sequence>
         <xs:attribute ref="No"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="mg">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="m"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tg"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="xg"/>
         </xs:sequence>
         <xs:attribute ref="No"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="m">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="head"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="semRel"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="defNative"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="head">
      <xs:complexType>
         <xs:choice maxOccurs="10" minOccurs="0">
            <xs:element ref="field"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="register"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="sem"/>
            <xs:element minOccurs="0" ref="sense"/>
            <xs:element minOccurs="0" ref="tags"/>
            <xs:element minOccurs="0" ref="argg"/>
            <xs:element name="range">
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="xs:string">
                        <xs:attribute name="rangeSpat">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="dialect"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="rangeTemp">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="neologism"/>
                                 <xs:enumeration value="obsolete"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="argg">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="arg"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="arg">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="typical"/>
         </xs:sequence>
         <xs:attribute name="name">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="obj"/>
                  <xs:enumeration value="subj"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="obj">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="animal"/>
                  <xs:enumeration value="human"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="subj">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="animal"/>
                  <xs:enumeration value="human"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute ref="cx"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="sem">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="isa"/>
         </xs:sequence>
         <xs:attribute ref="pos"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="sense">
      <xs:complexType>
         <xs:attribute name="sense">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="figurative"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="isa">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="isaType"/>
               <xs:attribute ref="lemmaID"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="defNative">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute name="form">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="definition"/>
                        <xs:enumeration value="gloss"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="src">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute name="value">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="ERV"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="xg">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="x"/>
            <xs:choice maxOccurs="unbounded">
               <xs:element ref="xt"/>
               <xs:element ref="xtCtn"/>
            </xs:choice>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="answer"/>
         </xs:sequence>
         <xs:attribute ref="genreType"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="x" type="xs:string"/>
   <xs:element name="answer">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="native"/>
            <xs:choice maxOccurs="unbounded">
               <xs:element maxOccurs="unbounded" ref="xt"/>
               <xs:element maxOccurs="unbounded" ref="tg"/>
            </xs:choice>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="native" type="xs:string"/>
   <xs:element name="tags" type="xs:string"/>
   <xs:element name="semRel">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element minOccurs="0" ref="lg"/>
            <xs:element minOccurs="0" ref="pos"/>
         </xs:sequence>
         <xs:attribute ref="drvType"/>
         <xs:attribute ref="lemmaID"/>
         <xs:attribute ref="pos"/>
         <xs:attribute name="semRelType">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="antonym"/>
                  <xs:enumeration value="confer"/>
                  <xs:enumeration value="hyponym"/>
                  <xs:enumeration value="hypernym"/>
                  <xs:enumeration value="meronym"/>
                  <xs:enumeration value="seeNative"/>
                  <xs:enumeration value="see"/>
                  <xs:enumeration value="seeAlso"/>
                  <xs:enumeration value="synonym"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="xt">
      <xs:complexType mixed="true">
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="lang"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="xtCtn">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="xt"/>
            <xs:element minOccurs="0" ref="re"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="tg">
      <xs:complexType>
         <xs:choice>
            <xs:element maxOccurs="unbounded" ref="t"/>
            <xs:element maxOccurs="unbounded" ref="tCtn"/>
         </xs:choice>
         <xs:attribute ref="lang"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="tCtn">
      <xs:complexType>
         <xs:choice>
            <xs:element maxOccurs="unbounded" ref="t"/>
            <xs:element maxOccurs="unbounded" ref="re"/>
         </xs:choice>
         <xs:attribute ref="lang"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="t">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute name="gen">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="f"/>
                        <xs:enumeration value="m"/>
                        <xs:enumeration value="n"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="nx"/>
               <xs:attribute ref="pos"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="field">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="fieldType"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="constituent">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="cx"/>
               <xs:attribute ref="dx"/>
               <xs:attribute ref="nx"/>
               <xs:attribute ref="pos"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <!-- simple content elements - text only -->
   <xs:element name="re" type="xs:string"/>
   <xs:element name="firstName" type="xs:string"/>
   <xs:element name="surname" type="xs:string"/>
   <xs:element name="stemForm" type="xs:string"/>
   <xs:element name="abbrUsed" type="xs:string"/>
   <xs:element name="decl" type="xs:string"/>
   <xs:element name="gov" type="xs:string"/>
   <xs:element name="i" type="xs:string"/>
   <xs:element name="typical">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:string">
               <xs:attribute ref="lemmaID"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <!-- Complex Empty Elements (attributes but no text or element content) -->
   <xs:element name="register">
      <xs:complexType>
         <xs:attribute name="register">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="colloquial"/>
                  <!--Umgangssprache Erz-Deu_2002-->
                  <xs:enumeration value="conversation"/>
                  <xs:enumeration value="dial"/>
                  <xs:enumeration value="motherese"/>
                  <xs:enumeration value="poetic"/>
                  <xs:enumeration value="vulgar"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <!-- -->
   <!-- ATTRIBUTES
-->
   <xs:attribute name="lang">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="arab"/>
            <!-- Arab?? -->
            <xs:enumeration value="de"/>
            <xs:enumeration value="ell"/>
            <!-- Modern Greek -->
            <xs:enumeration value="en"/>
            <xs:enumeration value="erz"/>
            <xs:enumeration value="es"/>
            <xs:enumeration value="fi"/>
            <xs:enumeration value="fr"/>
            <xs:enumeration value="grc"/>
            <!-- Ancient Greek -->
            <xs:enumeration value="a-iran"/>
            <!-- Ancient Iran?? -->
            <xs:enumeration value="kom"/>
            <xs:enumeration value="lat"/>
            <xs:enumeration value="mdf"/>
            <xs:enumeration value="ru"/>
            <xs:enumeration value="sme"/>
            <!-- Northern Sámi -->
            <xs:enumeration value="tr"/>
            <xs:enumeration value="tt"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="cx">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="abe"/>
            <xs:enumeration value="abl"/>
            <xs:enumeration value="abs"/>
            <xs:enumeration value="comp"/>
            <xs:enumeration value="comit"/>
            <xs:enumeration value="dat"/>
            <xs:enumeration value="ela"/>
            <xs:enumeration value="gen"/>
            <xs:enumeration value="ill"/>
            <xs:enumeration value="ine"/>
            <xs:enumeration value="lat"/>
            <xs:enumeration value="loc"/>
            <xs:enumeration value="nom"/>
            <xs:enumeration value="prol"/>
            <xs:enumeration value="temp"/>
            <xs:enumeration value="trnsl"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="dx">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="defPl"/>
            <xs:enumeration value="defSg"/>
            <xs:enumeration value="indef"/>
            <xs:enumeration value="poss"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="etymType">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="newForeignWord"/>
            <xs:enumeration value="oldForeignWord"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="nx" type="nxType"/>
   <xs:attribute name="possNx" type="nxType"/>
   <xs:attribute name="semNx" type="nxType"/>
   <xs:simpleType name="nxType">
      <xs:restriction base="xs:string">
         <xs:enumeration value="Pl"/>
         <xs:enumeration value="Sg"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:attribute name="genre">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="proverb"/>
            <xs:enumeration value="riddle"/>
            <xs:enumeration value="saying"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="constituentOrder">
      <xs:simpleType>
         <xs:restriction base="xs:integer">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
            <xs:enumeration value="4"/>
            <xs:enumeration value="5"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="drvType">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:enumeration value="DIM"/>
            <xs:enumeration value="Dim/не"/>
            <xs:enumeration value="Dim/ке"/>
            <xs:enumeration value="DV_N"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="der">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:pattern value="DimNE|DimKE|a|ad-a|adv|n|phrase-a|phrase-n|phrase-v|pop|pron|q|v"/>
         </xs:restriction>
      </xs:simpleType>
   </xs:attribute>
   <xs:attribute name="pos" type="posType"/>
   <xs:simpleType name="posType">
      <xs:restriction base="xs:string">
         <xs:enumeration value="A"/>
         <xs:enumeration value="AD-A"/>
         <xs:enumeration value="PRED_A"/>
         <xs:enumeration value="A-ADN"/>
         <xs:enumeration value="A-INDEF"/>
         <xs:enumeration value="V-GEN_A"/>
         <xs:enumeration value="ADV"/>
         <xs:enumeration value="CEK_ADV"/>
         <xs:enumeration value="NEK_ADV"/>
         <xs:enumeration value="ADV-GEN"/>
         <xs:enumeration value="ADV-IDEOPH"/>
         <xs:enumeration value="ADV-INTERR"/>
         <xs:enumeration value="MANNER"/>
         <xs:enumeration value="MANNER-INDEF"/>
         <xs:enumeration value="ADV-PARENTHETIC"/>
         <xs:enumeration value="PRT_ADV-PARENTHETIC"/>
         <xs:enumeration value="ADV-POSITION"/>
         <xs:enumeration value="SPAT-0ARG"/>
         <xs:enumeration value="SPAT-0ARG-INTERR"/>
         <xs:enumeration value="SPAT-INDEF"/>
         <xs:enumeration value="SPAT-DEM"/>
         <xs:enumeration value="SPAT-DISTR"/>
         <xs:enumeration value="ADV-SPAT-GEN"/>
         <xs:enumeration value="ADV-STATE"/>
         <xs:enumeration value="TEMP-0ARG"/>
         <xs:enumeration value="TEMP-0ARG-INDEF"/>
         <xs:enumeration value="TEMP-0ARG-INTERR"/>
         <xs:enumeration value="TEMP-0ARG-REL"/>
         <xs:enumeration value="TEMP-INDEF"/>
         <xs:enumeration value="TEMP-DEICT"/>
         <xs:enumeration value="ART"/>
         <xs:enumeration value="CLT"/>
         <xs:enumeration value="CONJ"/>
         <xs:enumeration value="COORD"/>
         <xs:enumeration value="COORD-CORRELATIVE"/>
         <xs:enumeration value="SUBORD"/>
         <xs:enumeration value="DEM-ADN"/>
         <xs:enumeration value="DEM-PRON"/>
         <xs:enumeration value="SOD"/>
         <!--Speaker-oriented determiner-->
         <xs:enumeration value="DET-PRON"/>
         <xs:enumeration value="DET-ADN"/>
         <xs:enumeration value="DET-PL"/>
         <xs:enumeration value="FORMULAIC"/>
         <xs:enumeration value="GEN-ATTR"/>
         <xs:enumeration value="ABE_A"/>
         <xs:enumeration value="GEN_A"/>
         <xs:enumeration value="GER"/>
         <xs:enumeration value="GER-ZJ_A"/>
         <xs:enumeration value="GER-ZJ_MANNER"/>
         <xs:enumeration value="GER-ZJ_N"/>
         <xs:enumeration value="GER-ZJ_STATE"/>
         <xs:enumeration value="IDEOPH"/>
         <xs:enumeration value="IDEOPH-INE"/>
         <xs:enumeration value="INTERJ"/>
         <xs:enumeration value="INTERJ-IMP"/>
         <xs:enumeration value="INTERJ-CONATIVE"/>
         <xs:enumeration value="PRT_INTERR"/>
         <xs:enumeration value="MODIFIER"/>
         <xs:enumeration value="DV_N"/>
         <xs:enumeration value="N"/>
         <xs:enumeration value="INE_N"/>
         <xs:enumeration value="NN"/>
         <xs:enumeration value="N-ASSOC"/>
         <xs:enumeration value="N-COLL"/>
         <xs:enumeration value="N-PL"/>
         <xs:enumeration value="N-SG"/>
         <xs:enumeration value="NUM"/>
         <xs:enumeration value="NUM-ADN"/>
         <xs:enumeration value="NUM-UNIV"/>
         <xs:enumeration value="CARD"/>
         <xs:enumeration value="CARD-ADV"/>
